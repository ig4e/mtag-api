{
    "$schema": "http://json.schemastore.org/swcrc",
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "decorators": true,
        "dynamicImport": true
      },
  
      "target": "esnext",
      "baseUrl": "./src",
      "paths": {
        "@/*": ["./*"]
      }
    },
    "module": {
      "type": "commonjs"
    },
    "exclude": ["node_modules"]
  }
  